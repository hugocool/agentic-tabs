{
  "name": "agentic-tabs",
  "version": "0.1.0",
  "private": true,
  "description": "Agentic tab triage extension (Plasmo + LangGraph + Notion)",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "manifest": {
    "permissions": ["tabs", "tabGroups", "sessions", "storage", "activeTab"],
    "host_permissions": ["https://api.notion.com/*"],
    "chrome_url_overrides": { "newtab": "newtab.html" },
    "action": {
      "default_title": "Agentic Tabs"
    }
  },
  "dependencies": {
    "@langchain/core": "^0.2.0",
    "@langchain/langgraph": "^0.0.32",
    "@notionhq/client": "^2.2.14",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.262",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "plasmo": "^0.87.2",
    "typescript": "^5.4.5"
  }
}
