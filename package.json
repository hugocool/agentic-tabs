{
  "name": "agentic-tabs",
  "version": "0.1.0",
  "packageManager": "pnpm@8.15.8",
  "private": true,
  "description": "Agentic tab triage extension (Plasmo + LangGraph + Notion)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-user/agentic-tabs.git"
  },
  "bugs": {
    "url": "https://github.com/your-user/agentic-tabs/issues"
  },
  "homepage": "https://github.com/your-user/agentic-tabs#readme",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "manifest": {
    "permissions": [
      "activeTab",
      "notifications",
      "sessions",
      "storage",
      "tabGroups",
      "tabs"
    ],
    "host_permissions": [
      "https://api.notion.com/*"
    ],
    "chrome_url_overrides": {
      "newtab": "newtab.html"
    },
    "options_ui": {
      "page": "options.html",
      "open_in_tab": true
    },
    "action": {
      "default_title": "Agentic Tabs"
    },
    "commands": {
      "start-session": {
        "description": "Start a new session"
      },
      "preview-triage": {
        "description": "Preview triage"
      },
      "resume-last-session": {
        "description": "Resume last session"
      },
      "open-options": {
        "description": "Open options"
      },
      "focus-manager": {
        "description": "Focus Manager tab"
      }
    }
  },
  "engines": {
    "node": ">=20 <23"
  },
  "dependencies": {
    "@langchain/langgraph": "^0.4.9",
    "@notionhq/client": "^5.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "uuid": "^9.0.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/chrome": "^0.1.6",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/uuid": "^10.0.0",
    "plasmo": "0.87.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}